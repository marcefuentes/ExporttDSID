{
  "manifest_version": 2,
  "name": "BU VPM DSID Extractor",
  "description": "Extract ssl.binghamton.edu DSID cookie to file",
  "version": "0.1.0",
  "icons": {
    "128": "icon/icon_128.png"
  },
  "browser_action": {
    "default_icon": {
    "128": "icon/icon_128.png"
    },
    "default_title": "DSIDExtractor",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "listener.js"
    ],
    "persistent": false
  },
 "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs",
    "cookies",
    "https://*/*"
  ]
}
